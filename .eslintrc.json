{
    "extends": [
      "@croquiscom/eslint-config-www"
    ],
    "rules": {
      "no-restricted-imports": "off",
      "no-restricted-globals": "off",
      "react/prop-types": "off",
      "react/display-name": "off",
      "react/no-unescaped-entities": "off",
      "no-script-url": "off",
      "react-hooks/exhaustive-deps": "off",
      "no-shadow": "off"
    }
  }